package linkedlist;

public class SinglyLinkedList {
	Node head;
	Node tail;
		
	public SinglyLinkedList(Node node) {
		this.head = node;
		this.tail = node;
	}
	public Node getHead() {
		return head;
	}
	public void setHead(Node head) {
		this.head = head;
	}
	public Node getTail() {
		return tail;
	}
	public void setTail(Node tail) {
		this.tail = tail;
	}
	
	public void add(Node n) {
		tail.setNext(n);
		n = tail;
	}
}
