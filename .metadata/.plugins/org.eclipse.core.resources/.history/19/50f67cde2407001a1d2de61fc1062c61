package linkedlist;

public class ListMain {

	public static void main(String[] args) {
		Node n = new Node("Value");
		SinglyLinkedList list = new SinglyLinkedList(n);
		
		Node n1 = new Node((Integer) 12);
		list.add(n1);
		Node n2 = new Node((Double) 36.7);
		list.add(n2);
		Node n3 = new Node((Boolean) true);
		list.add(n3);
		Node n4 = new Node((String) "Chen");
		list.add(n4);
		Node n5 = new Node((Integer) 43);
		list.add(n5);
//		Node n6 = new Node(38);
//		list.insertAt(n6, 3);
		
		list.printList();
		

	}

}
