package capitalize;

public class Capitalize {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		public static void capitalizeEach(String s) {
			String[] arr = s.split(" ");
			for(String str : arr) {
				str = str.substring(0, 1).toUpperCase() + str.substring(1);
			}
		}
	}

}
